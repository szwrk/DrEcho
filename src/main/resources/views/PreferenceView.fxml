<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.VBox?>
<?import java.lang.String?>
<TitledPane xmlns:fx="http://javafx.com/fxml/1" collapsible="false" maxHeight="1.7976931348623157E308"
            maxWidth="1.7976931348623157E308" text="%ui.initsettings.title" xmlns="http://javafx.com/javafx/21"
            fx:controller="net.wilamowski.drecho.client.presentation.settings.PreferenceController">
    <content>
        <VBox fx:id="root" alignment="TOP_CENTER">
            <children>
                <TableView fx:id="table" editable="true" maxHeight="1.7976931348623157E308"
                           maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
                    <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </VBox.margin>
                    <styleClass>
                        <String fx:value="dense"/>
                        <String fx:value="striped"/>
                    </styleClass>
                </TableView>
            </children>
        </VBox>
    </content>
</TitledPane>
