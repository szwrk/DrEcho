<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="examination" prefHeight="258.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="net.wilamowski.drecho.client.presentation.examinations.chooser.ExaminationsChooserController">
   <children>
      <VBox alignment="CENTER" maxHeight="25.0" minHeight="25.0" prefHeight="25.0">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="19.0" prefWidth="989.0" styleClass="title-3" text="Planowanie badań" textAlignment="CENTER">
               <font>
                  <Font name="System Bold" size="13.0" />
               </font>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </Label>
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </VBox>
      <Label prefHeight="20.0" prefWidth="866.0" styleClass="text-subtle" text="Wskazówka: wybierz z katalogu usług badania, które chcesz wykonać">
         <VBox.margin>
            <Insets bottom="5.0" left="10.0" right="5.0" top="5.0" />
         </VBox.margin>
      </Label>
      <VBox prefHeight="230.0" prefWidth="1000.0" VBox.vgrow="ALWAYS">
         <children>
            <HBox prefHeight="206.0" prefWidth="990.0" VBox.vgrow="ALWAYS">
               <children>
                  <VBox prefHeight="210.0" prefWidth="482.0" HBox.hgrow="ALWAYS">
                     <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </VBox.margin>
                     <children>
                        <Label prefHeight="24.0" prefWidth="442.0" styleClass="text-caption" text="Katalog usług">
                           <VBox.margin>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                           </VBox.margin>
                           <graphic>
                              <ImageView>
                                 <image>
                                    <Image url="@../../assets/dictionary.png" />
                                 </image>
                              </ImageView>
                           </graphic>
                        </Label>
                        <TableView fx:id="examinationsDictionaryTable" prefHeight="176.0" prefWidth="490.0" styleClass="striped" VBox.vgrow="ALWAYS">
                           <columns>
                              <TableColumn fx:id="codeDictionaryColumn" minWidth="50.0" prefWidth="75.0" text="Kod" />
                              <TableColumn fx:id="nameDictionaryColumn" minWidth="200.0" prefWidth="-1.0" text="Nazwa" />
                              <TableColumn fx:id="definitionActionColumn" minWidth="50.0" prefWidth="75.0" text="Akcja" />
                           </columns>
                           <columnResizePolicy>
                              <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                           </columnResizePolicy>
                        </TableView>
                     </children>
                     <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </HBox.margin>
                  </VBox>
                  <HBox alignment="CENTER" prefHeight="158.0" prefWidth="28.0" />
                  <VBox HBox.hgrow="ALWAYS">
                     <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="20.0" />
                     </VBox.margin>
                     <children>
                        <Label prefHeight="24.0" prefWidth="468.0" styleClass="text-caption" text="Usługi wybrane do realizacji podczas wizyty">
                           <VBox.margin>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                           </VBox.margin>
                           <graphic>
                              <ImageView>
                                 <image>
                                    <Image url="@../../assets/checklist.png" />
                                 </image>
                              </ImageView>
                           </graphic>
                        </Label>
                        <TableView fx:id="selectedExhaminationsTable" prefHeight="193.0" prefWidth="494.0" styleClass="striped" VBox.vgrow="ALWAYS">
                           <columns>
                              <TableColumn fx:id="selectedExhaminationTempIdColumn" maxWidth="100.0" minWidth="35.0" prefWidth="35.0" text="LP" />
                              <TableColumn fx:id="selectedExhaminationCodeColumn" maxWidth="100.0" minWidth="50.0" prefWidth="75.0" text="Kod" />
                              <TableColumn fx:id="selectedExhaminationNameColumn" maxWidth="500.0" minWidth="200.0" prefWidth="-1.0" text="Nazwa" />
                              <TableColumn fx:id="selectedExaminationEditColumn" maxWidth="60.0" minWidth="60.0" prefWidth="60.0" text="Edytuj" />
                              <TableColumn fx:id="selectedExaminationPrintColumn" maxWidth="60.0" minWidth="60.0" prefWidth="60.0" text="Drukuj" />
                              <TableColumn fx:id="selectedExaminationDeleteColumn" maxWidth="60.0" minWidth="60.0" prefWidth="60.0" text="Usuń" />
                           </columns>
                           <columnResizePolicy>
                              <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                           </columnResizePolicy>
                        </TableView>
                     </children>
                     <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </HBox.margin>
                  </VBox>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </children>
</VBox>
